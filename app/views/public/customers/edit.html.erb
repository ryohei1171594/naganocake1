<h2>会員情報</h2>
<%= form_with model: @customer, url: public_customer_path(@customer), method: :patch do |f| %>
  <label for="inputLastName">LastNaame</label>
  <%= f.text_field :last_name, autofocus: true, id:"inputLastName", placeholder:"姓"%>
  <label for="inputFirstName">FirstNaame</label>
  <%= f.text_field :first_name, autofocus: true, id:"inputFirstName", placeholder:"名"%>
  <label for="inputLastName">LastNaameKana</label>
  <%= f.text_field :last_name_kana, autofocus: true, id:"inputLastNameKama", placeholder:"姓カナ"%>
  <label for="inputFistNameKana">FirstNaameKana</label>
  <%= f.text_field :first_name_kana, autofocus: true, id:"inputFirstNameKana", placeholder:"名カナ"%>
  <label for="inputPostalCode">PostalCode</label>
  <%= f.text_field :postal_code, autofocus: true, id:"inputPostalCode", placeholder:"郵便番号"%>
  <label for="inputAddress">Address</label>
  <%= f.text_field :address, autofocus: true, id:"inputAddress", placeholder:"住所"%>
  <label for="inputTelephoneNumber">TelephoneNumber</label>
  <%= f.text_field :telephone_number, autofocus: true, id:"inputTelephoneNumber", placeholder:"電話番号"%>
  <label for="inputLastEmail">Email</label>
  <%= f.text_field :email, autofocus: true, id:"inputEmail", placeholder:"email"%>
  <%= f.submit "変更を保存" %>
<% end %>
